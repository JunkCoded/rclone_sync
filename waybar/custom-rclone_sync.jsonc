{
    "custom/rclone_sync": {
        "format": "{} ",
        "return-type": "json",
        "exec": "~/.rclone_sync/waybar_rclone.sh status",
        "interval": 5,
        "signal": 8,
        "tooltip": true,
        "on-click": "~/.rclone_sync/waybar_rclone.sh sync",
        "on-click-right": "~/.rclone_sync/waybar_rclone.sh toggle-sync"
    }
}
